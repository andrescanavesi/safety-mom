

<h1 class="my-4">{{pageHeader}}</h1> <!-- used to display categories such as tags -->
{{#each posts}}
	<!-- Blog Post -->
	<div class="container">
		<div class="row">
			<div class="col align-middle">
				<img class="lozad card-img-top mt-3"
				     src="{{this.default_loading_image}}"
				     data-src="{{this.featured_image_url}}" alt="image post">
			</div>
			<div class="col-8">
				<h1>
					<a href="/post/{{this.title_seo}}">{{this.title}}</a>
				</h1>
				<h4 class="d-none d-md-block">
					<small class="text-muted lead hidden-lg-up">{{this.sub_title}}</small>
				</h4>
			</div>

		</div>
		<div class="row">
			<div class="col">
				<p class="lead text-justify mt-3">{{this.summary}}</p>
				<a href="/post/{{this.title_seo}}" ><span class="text-success">Seguir leyendo - 3 min de lectura</span></a>

				<!-- post metadata -->
				<div class="d-flex mt-1">
					<div class="p-2 d-none">
						<a href="#">
							<img alt="heart icon" class="socialShareIcon" src="data:image/png;base64,">
						</a>
					</div>
					<div class="ml-auto p-2">
						<a href="{{this.whatsappSharingUrl}}" class="mr-2">
							<img alt="whatsapp icon" class="socialShareIcon" src="data:image/svg+xml;base64,">
						</a>
						<a href="{{this.facebookSharingUrl}}" class="mr-2">
							<img alt="facebook icon" class="socialShareIcon" src="data:image/svg+xml;base64,">
						</a>
						<a href="{{this.pinterestSharingUrl}}" class="mr-2">
							<img alt="pinterest icon" class="socialShareIcon" src="data:image/svg+xml;base64,">
						</a>
						<a href="{{this.linkedinSharingUrl}}" class="mr-2">
							<img alt="linkedin icon" class="socialShareIcon" src="data:image/png;base64,">
						</a>
						<a href="{{this.twitterSharingUrl}}">
							<img alt="twitter icon" class="socialShareIcon" src="data:image/svg+xml;base64,">
						</a>
					</div>
				</div>
				<!-- END Post meta -->

			</div>
		</div>
	</div>
	<hr/>
{{/each}}

